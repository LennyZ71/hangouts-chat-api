{
  "name": "hangouts-chat-api",
  "version": "0.3.0",
  "description": "Node.js wrapper for Google's Hangouts Chat REST API",
  "keywords": [
    "hangouts",
    "chat",
    "api",
    "alerts",
    "notifications"
  ],
  "homepage": "https://github.com/mwpeterson/hangouts-chat-api",
  "bugs": {
    "url": "https://github.com/mwpeterson/hangouts-chat-api/issues"
  },
  "author": "Michael Peterson",
  "repository": {
    "type": "git",
    "url": "https://github.com/mwpeterson/hangouts-chat-api.git"
  },
  "files": [
    "src",
    "CONTRIBUTING.md"
  ],
  "main": "src/HangoutsChatNotification.js",
  "scripts": {
    "eslint": "eslint . --format=node_modules/eslint-formatter-pretty",
    "lint": "npm run npmpackagejsonlint && npm run eslint",
