    "request": "^2.83.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.14.0",
    "eslint-formatter-pretty": "^1.3.0",
    "npm-package-json-lint": "^2.11.0",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.14.7"
  },
